namespace CustomBuffs
    import Collections
    //伤害加深，拥有buff的单位受到伤害翻倍
    space class AmplifyDamageBuff Buff
        public OnCreate()
            owner.onHitEvent.Add(OnOwnerDmg)
        public OnRemove()
            owner.onHitEvent.Remove(OnOwnerDmg)
        private real OnOwnerDmg(Unit, real dmg)
            return dmg * 2