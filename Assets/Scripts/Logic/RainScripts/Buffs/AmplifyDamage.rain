namespace CustomBuffs
    import Collections
    //伤害加深，拥有buff的单位受到伤害翻倍
    space class AmplifyDamageBuff Buff
        public OnCreate()
            owner.onHitEvent += HitEvent& OnOwnerDmg
        public OnRemove()
            owner.onHitEvent += HitEvent& OnOwnerDmg
        private real OnOwnerDmg(Unit source, real dmg)
            return dmg * 2