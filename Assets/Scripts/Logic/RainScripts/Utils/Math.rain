namespace Utils
    import kernel.Math
    public real3 DirToForward(real dir)
        var s, var c = SinCos(dir)
        return real3(c, 0, s)
    public real ForwardToDir(real3 forward)
        return Atan2(forward.z, forward.x)
    public real2 Rotation(real2 v, real a)
        var s, var c = SinCos(a)
        return real2(v.x * c - v.y * s, v.x * s + v.y * c)